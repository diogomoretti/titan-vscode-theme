{
  "name": "Titan Theme",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Comment",
      "scope": [
        "comment",
        "string.comment"
      ],
      "settings": {
        "foreground": "--color-comments"
      }
    },
    {
      "name": "String",
      "scope": "string",
      "settings": {
        "foreground": "--color-accent"
      }
    },
    {
      "name": "js template-expression",
      "scope": "punctuation.definition.template-expression.begin.js,punctuation.definition.template-expression.end.js,punctuation.definition.template-expression.begin.ts,punctuation.definition.template-expression.end.ts",
      "settings": {
        "foreground": "--color-red"
      }
    },
    {
      "name": "TemplateString",
      "scope": [
        "string.template.js",
        "variable.parameter.js"
      ],
      "settings": {
        "foreground": "--color-accent"
      }
    },
    {
      "name": "TemplateString",
      "scope": "meta.template.expression.js",
      "settings": {
        "foreground": "--color-gray"
      }
    },
    {
      "name": "Number",
      "scope": "constant.numeric",
      "settings": {
        "foreground": "--color-2"
      }
    },
    {
      "name": "Embeded String Begin and End",
      "scope": [
        "string.embedded.begin",
        "string.embedded.end"
      ],
      "settings": {
        "foreground": "--color-2"
      }
    },
    {
      "name": "Embeded String",
      "scope": "string.embedded",
      "settings": {
        "foreground": "--color-4"
      }
    },
    {
      "name": "Built-in constant",
      "scope": "constant.language",
      "settings": {
